{
  "name": "bilibili-live-ws",
  "version": "5.1.2",
  "description": "Bilibili Live WebSocket/TCP API",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "npm run clear; npm run tsc && npm run unit",
    "unit": "nyc mocha --reporter=landing -r ts-node/register test/test.ts",
    "clear": "rm index.js index.d.ts src/*.js src/*.d.ts test/*.js test/*.d.ts;exit 0",
    "tsc": "tsc -b",
    "coverage": "mkdir coverage; nyc report --reporter=text-lcov >> ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simon300000/bilibili-live-ws.git"
  },
  "keywords": [
    "bilibili",
    "api",
    "websocket",
    "live",
    "ws",
    "tcp"
  ],
  "author": "simon3000 <simon3000@163.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simon300000/bilibili-live-ws/issues"
  },
  "homepage": "https://github.com/simon300000/bilibili-live-ws#readme",
  "dependencies": {
    "isomorphic-ws": "^4.0.1",
    "ws": "^8.2.3"
  },
  "optionalDependencies": {
    "got": "^11.1.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "@types/ws": "^7.2.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "got": "^11.1.4",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  }
}
